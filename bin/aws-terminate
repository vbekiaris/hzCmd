#!/usr/bin/env bash

boxFiles="$@"

if [ $# -eq 0 ]; then
    boxFiles="a.box"
fi

for boxFile in "${boxFiles}"; do

 lines=($(cat ${boxFile}))

 for line in "${lines[@]}" ; do

  id=$(echo ${line} | awk -F',' '{print $3}' )
  aws ec2 terminate-instances --instance-ids ${id}

 done

 rm ${boxFile}

done